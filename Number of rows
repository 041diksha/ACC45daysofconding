#include <stdio.h>

void main() 

{

int rows, n = 1, i, j;

   printf("Enter the number of rows: ");

   scanf("%d",&rows);

   for (i = 0; i<rows; i++) 

   {

      for (j = 0; j <= i; j++)

	{

         if (j == 0 || i == 0)

            n = 1;

         else

            n = n * (i - j + 1) / j;

         printf("%d",n);

      }

      printf("\n");

   }

}
